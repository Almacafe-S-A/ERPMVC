    <script>
        function error_handler(e) {
            var notification = $("#notification").data("kendoNotification");
            if (e.status == "customerror") {
                notification.show({
                    title: "Validacion",
                    message: e.errors
                }, "error");
            }
            else {
                notification.show({
                    title: "Validacion",
                    message: "Generic server error."
                }, "error");
            }
        }
        kendo.culture("es-HN");
    </script>
