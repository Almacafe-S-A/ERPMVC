            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <script>
                function MiZoom() {
                    var scale = 'scale(1)';
                    document.body.style.webkitTransform =  // Chrome, Opera, Safari
                        document.body.style.msTransform =      // IE 9
                        document.body.style.transform = scale;
                    // alert('hace el zoom');
                }
            </script>

            <div class="container-fluid">
