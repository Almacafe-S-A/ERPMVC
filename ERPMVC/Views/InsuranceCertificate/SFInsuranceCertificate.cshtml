@model ERPMVC.Models.InsuranceCertificate

@using Syncfusion.JavaScript

<div style=" align-items:center; height:825px;width: 100%;">
    <ej-report-viewer id="reportviewer1" rendering-begin="renderingBegin" report-print="onPrintReport" processing-mode="Remote" locale="es-ES"
report-service-url="/ReportViewer"  report-loaded="onReportLoaded" report-path="/ReportsTemplate/SegurosEndosadosPoliza.rdl" />

</div>




<script type="text/javascript">

    function onReportLoaded(event) {
        var parameters = [{
            name: 'InsuranceCertificateId',
            labels: ['@Model.Id'],
            values: [@Model.Id],
            nullable: false
        }
        ];
        event.data = parameters;
    }; 

    

</script>


<ej-script-manager></ej-script-manager>