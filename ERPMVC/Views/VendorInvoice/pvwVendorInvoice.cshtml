     function SaveFacturaProveedores(e) {
        debugger;
        var notification = $("#notification").data("kendoNotification");
        // e.preventDefault();
        //$("#btnSaveVendorInvoice").hide();
        $("#btnSaveVendorInvoice").prop("disabled", true);

        var validadetalle = true;
        var displayedData = $("#gridInvoiceDetail").data().kendoGrid.dataSource.view();
        for (var i = 0; i < displayedData.length; i++) {
            displayedData[i].VendorInvoiceLineId = 0;
            if (displayedData[i].TaxCode === '' || displayedData[i].TaxCode === null
                || displayedData[i].Total === 0) {
                notification.show({
                    title: "Validación",
                    message: "No Ingreso el codigo de impuesto"
                }, "error");

                validadetalle = false;
            }
        }

         if (validadetalle) {
             var _receivedDate = "0001-01-01";
             var _orderDate = "0001-01-01";
             var _expirationDate = "0001-01-01";
             if ($("#ReceivedDate").getKendoDateTimePicker().value() != null || $("#ReceivedDate").getKendoDateTimePicker().value() != undefined) {
                 _receivedDate = $("#ReceivedDate").getKendoDateTimePicker().value().toJSON()
             }
             if ($("#OrderDate").getKendoDateTimePicker().value() != null || $("#OrderDate").getKendoDateTimePicker().value() != undefined) {
                 _orderDate = $("#OrderDate").getKendoDateTimePicker().value().toJSON()
             }
             if ($("#ExpirationDate").getKendoDateTimePicker().value() != null || $("#ExpirationDate").getKendoDateTimePicker().value() != undefined) {
                 _expirationDate = $("#ExpirationDate").getKendoDateTimePicker().value().toJSON()
             }
            var dataObject = {
                'VendorInvoiceId': $("#VendorInvoiceId").val() === "" ? "0" : $("#VendorInvoiceId").val(),
                'VendorInvoiceName': $("#VendorInvoiceName").val(),
                'PurchaseOrderId': $("#PurchaseOrderId").val(),
                'BranchId': $("#BranchId").val(),
                'BranchName': $("#BranchId").data("kendoDropDownList").text(),
                'VendorId': $("#VendorId").val(),
                'VendorName': $("#VendorId").data("kendoDropDownList").text(),
                'OrderDate': _orderDate,
                'VendorInvoiceDate': _orderDate,
                'ReceivedDate': _receivedDate,
                'ExpirationDate': _expirationDate,
                'CurrencyId': Number($("#CurrencyId").val()),
                'CurrencyName': $("#CurrencyId").data("kendoDropDownList").text(),
                'VendorRefNumber': $("#VendorRefNumber").val(),
                'SalesTypeId': $("#SalesTypeId").val(),
                'NumeroDEI': $("#NumeroDEI").val(),
                //'NoOCExenta': $("#NoOCExenta").val(),
                //'NoSAG': $("#NoSAG").val(),
                //'NoConstanciadeRegistro': $("#NoConstanciadeRegistro").val(),
                'Remarks': $("#Remarks").val(),
                'Amount': $("#Amount").val(),
                'SubTotal': $("#SubTotal").val(),
                'Discount': $("#Discount").val(),
                'Tax': $("#Tax").val(),
                    'Tax18': $("#Tax18").val(),
                'Freight': $("#Freight").val(),
                'Total': $("#Total").val(),
                'Correo': $("#Correo").val(),
                'Direccion': $("#Direccion").val(),
