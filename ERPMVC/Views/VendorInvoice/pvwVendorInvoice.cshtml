                                                                                     , UnitOfMeasureName: data.SalesOrderLines[i].UnitOfMeasureName, UnitOfMeasureId: data.SalesOrderLines[i].UnitOfMeasureId
                                                                                     //, ProductId: data.SalesOrderLines[i].ProductId, SubProductName: data.SalesOrderLines[i].SubProductName
                                                                                     , SubTotal: data.SalesOrderLines[i].SubTotal, Price: data.SalesOrderLines[i].Price
                                                                                     , DiscountAmount: data.SalesOrderLines[i].DiscountAmount, TaxCode: data.SalesOrderLines[i].TaxCode
                                                                                     , TaxAmount: data.SalesOrderLines[i].TaxAmount, Total: data.SalesOrderLines[i].Total
                                                                                     , Quantity: data.SalesOrderLines[i].Quantity, WareHouseId: data.SalesOrderLines[i].WarehouseId
                                                                                     , ControlPalletsId: $("#cbxBuscarCotizaciones").val()
                                                                                 });
                                                                             }
                                                                         }

                                                                       //  $("#singuardar").val(1);


                                                                     },
                                                                     error: function (XMLHttpRequest, textStatus, errorThrown) {

                                                                       //  $("#btngenerarfactura").show();
                                                                         notification.show({
