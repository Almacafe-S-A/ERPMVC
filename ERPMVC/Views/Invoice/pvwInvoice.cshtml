<script>
    function validar() {
        var cadena = $("#Tefono").val();
        if (cadena.indexOf("_") > -1 == true) {
            $("#telefono").show();
        }
        else {
            $("#telefono").hide();
        }
    }
</script>

<script>

    function GetClienteService() {
        return {
            CustomerId: $("#CustomerId").val(),
            Service: $("#ProductId").val(),
        };
    }
    function Numeros(e) {
        var key = window.Event ? e.which : e.keyCode
        return (key >= 48 && key <= 57)
    }

    function GetProductType(){
        return { ProductType:1}
    }

    $(document).ready(function () {
        $("#Tefono").kendoMaskedTextBox({
            mask: "(999) 0000-0000"
        });

        $("#telefono").hide();

        var cai = $("#CAI").val();
        if (cai === null || cai === ''){
            $("#datosfiscales").hide();
            //$("#datosExonerado").hide();
            
        }else{
            $("#datosExonerado").show();
            $("#datosfiscales").show();
        }
        var exo = $("#Exonerado").data("kendoSwitch");
        var exonerado = exo.check();
        
    });
    function SoloNumeros(e) {
        key = e.keyCode || e.which;
        tecla = String.fromCharCode(key).toLowerCase();
        letras = "1234567890.";
        especiales = "8-37-39-46";
        tecla_especial = false
        for (var i in especiales) {
            if (key == especiales[i]) {
                tecla_especial = true;
                break;
            }
        }
        if (letras.indexOf(tecla) == -1 && !tecla_especial) {
            return false;
        }
    }


    function GetProductIdS() {
        return { 'ProductId': $("#ProductId").val() }
    }


    function RefreshCotizaciones() {
        var grid = $("#gridInvoices").getKendoGrid();
        grid.dataSource.read();
    }

    function SaveCotizacion(e) {
        try {
            $("#btnSaveInvoice").prop("disabled", true);
            var validadetalle = true;
            var displayedData = $("#gridInvoiceDetail").data().kendoGrid.dataSource.view();
            if (displayedData.length <= 0) {
                MostrarMensaje('Debe agregar los productos!', "Error", "Validación", 6000);
            }
            for (var i = 0; i < displayedData.length; i++) {
                if (displayedData[i].Total == 0) {
                    MostrarMensaje('No pueden ir datos vacios en el detalle la factura', "Error", "EValidaciónrror", 6000);
                    return;
                }
            }
            var id = 0;
            var validator = $("#frmSalesOrder").data("kendoValidator");
            var status = $(".status");

            if (!validator.validate()) {
                MostrarMensaje('Datos incorrectos en el formulario!', "Error", "Validación", 6000);
                $("#btnSaveInvoice").show();
                $("#btnSaveInvoice").prop("disabled", false);
            }

            var exo = $("#Exonerado").data("kendoSwitch");
            var exonerado =  exo.check();

            var exe = $("#Exento").data("kendoSwitch");
            var exento =  exe.check();

            var dataObject = {
                'InvoiceId': $("#InvoiceId").val() === "" ? "0" : $("#InvoiceId").val(),
                'InvoiceName': $("#InvoiceName").val(),
                'BranchId': $("#BranchId").val(),
                'CustomerContractId': $("#CustomerContractId").val(),
                'BranchName': $("#BranchId").data("kendoDropDownList").text(),
                'CustomerId': $("#CustomerId").val(),
                'CustomerName': $("#CustomerId").data("kendoDropDownList").text(),
                'ProductId': $("#ProductId").val(),
                'ProductName': $("#ProductId").data("kendoDropDownList").text(),
                'OrderDate': $("#OrderDate").getKendoDateTimePicker().value().toJSON(),
                'InvoiceDate': $("#OrderDate").getKendoDateTimePicker().value().toJSON(),
                'ExpirationDate': $("#ExpirationDate").getKendoDateTimePicker().value().toJSON(),
                'SalesTypeId': $("#SalesTypeId").val(),
                'NoOCExenta': $("#NoOCExenta").val(),
                'NoSAG': $("#NoSAG").val(),
                'NoConstanciadeRegistro': $("#NoConstanciadeRegistro").val(),
                'Remarks': $("#Remarks").val(),
                'Correo': $("#Correo").val(),
                'Direccion': $("#Direccion").val(),
                'RTN': $("#RTN").val(),
                'Estado': $("#Estado").val(),
                'Tefono': $("#Tefono").val(),
                'TotalExento': $("#TotalExento").val(),
                'TotalExonerado': $("#TotalExonerado").val(),
                'NumeroDEI': $('#NumeroDEI').val(),
                'CAI': $('#CAI').val(),
                'Sinopsis': $('#Sinopsis').val(),
