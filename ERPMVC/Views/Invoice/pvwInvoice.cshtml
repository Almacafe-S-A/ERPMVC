                                                        <messages checked="Si" unchecked="No" />
                                                    </kendo-switch>
                                                </div>
                                            </div>

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label" style="min-width:100%">Exento</label>
                                            <kendo-switch name="Exento" for="Exento" checked="@Model.Exento" on-change="onChangeExonerado">
                                                <messages checked="Si" unchecked="No" />
