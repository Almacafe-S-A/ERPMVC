                                                        var grid = $("#gridGoodsDeliveredDetail").data("kendoGrid");
                                                        var datasource = grid.dataSource;
                                                        var raw = datasource._data;
                                                        for (i = datasource._data.length - 1; i >= 0; i--) {
                                                            item = raw[i];
                                                            datasource.remove(item);
                                                        }

                                                        for (i = 0; i < data.GoodsDeliveryAuthorizationLine.length; i++) {
                                                            datasource.insert({
                                                                GoodsDeliveredLinedId: 0
                                                                , GoodsDeliveredId: 0
                                                                , NoCD: data.GoodsDeliveryAuthorizationLine[i].NoCertificadoDeposito
                                                                , UnitOfMeasureName: data.GoodsDeliveryAuthorizationLine[i].UnitOfMeasureName
                                                                , UnitOfMeasureId: data.GoodsDeliveryAuthorizationLine[i].UnitOfMeasureId
