@model ERPMVC.DTO.GoodsReceivedDTO

@using Syncfusion.JavaScript

<div style=" align-items:center; height:825px;width: 100%;">
    <ej-report-viewer id="reportviewer1" rendering-begin="renderingBegin" processing-mode="Remote" ajax-before-load="ajaxBeforeLoad" report-service-url="/ReportViewer"  report-loaded="onReportLoaded" report-path="/ReportsTemplate/GoodsReceived.rdl" />
</div>


<script type="text/javascript">

    function ajaxBeforeLoad(event) {
        var parameters = [{
            name: 'GoodsReceivedId',
            labels: ['@Model.GoodsReceivedId'],
            values: [@Model.GoodsReceivedId],
            nullable: false
        }
        ];
        event.data = parameters;
    }; 

    function onReportLoaded(sender) {
        var reportObj = $('#reportviewer1').data("ejReportViewer");
        reportObj.model.parameters = [{
            name: 'GoodsReceivedId',
            labels: ['@Model.GoodsReceivedId'],
            values: ['@Model.GoodsReceivedId'],
            nullable: false
        }
        ];
    }

</script>


<ej-script-manager></ej-script-manager>