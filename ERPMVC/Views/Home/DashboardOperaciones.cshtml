

<script>

    $(document).ready(function () {

         $.ajax({
                url: '@Url.Action("GetQuantityIngresos", "DashBoard")',
                method: 'GET',
                datatype: "json",
                contentType: 'application/json',
                async: false,
               // data: dataObject,
                 success: function (result) {
                     //console.log('Cotizaciones'+result);
                     $("#Entradas").text(result);


                  },
                 error: function (XMLHttpRequest, textStatus, errorThrown) {
                        alert(textStatus + ": " + XMLHttpRequest.responseText);
                  }
        });


         $.ajax({
                url: '@Url.Action("GetQuantitySalidas", "DashBoard")',
                method: 'GET',
                datatype: "json",
                contentType: 'application/json',
                async: false,
               // data: dataObject,
                 success: function (result) {
                     //console.log('Cotizaciones'+result);
                     $("#Salidas").text(result);


                  },
                 error: function (XMLHttpRequest, textStatus, errorThrown) {
                        alert(textStatus + ": " + XMLHttpRequest.responseText);
                  }
               });

         $.ajax({
                url: '@Url.Action("GetQuantityAreasOcupadas", "DashBoard")',
                method: 'GET',
                datatype: "json",
                contentType: 'application/json',
                async: false,
               // data: dataObject,
                 success: function (result) {
                     //console.log('Cotizaciones'+result);
                     $("#AreasOcupadas").text(result);


                  },
                 error: function (XMLHttpRequest, textStatus, errorThrown) {
                        alert(textStatus + ": " + XMLHttpRequest.responseText);
                  }
               });


    });





    function error_handler(e) {
        var errors = $.parseJSON(e.xhr.responseText);

        if (errors) {
            alert("Errors:\n" + errors.join("\n"));
        }
    }



</script>


