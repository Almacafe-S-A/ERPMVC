    function GenerarEdad() {
        //if () {
        debugger;
        if ($("#FechaNacimiento").val() != "") {
            console.log("Fecha N", $("#FechaNacimiento").getKendoDatePicker().value());
            //if ($("#FechaNacimiento").getKendoDatePicker().value() != null) {
            //    var fecha = $("#FechaNacimiento").getKendoDatePicker().value().toJSON()
            //}
            //else {
                var fechaN = $("#FechaNacimiento").val() // 10/11/2009
                var anio = String(fechaN).substring(6, 10); //2009
                var dia = String(fechaN).substring(0, 2); //10
                var mes = String(fechaN).substring(3, 5); //11

                var FechaFormatiada = mes + '/' + dia + '/' + anio
                var fecha = new Date(FechaFormatiada).toJSON();
            //}
            var fecha_value = new Date(fecha).getFullYear(fecha);
            var fecha_valuem = new Date(fecha).getMonth(fecha) + 1;
            var fecha_valued = new Date(fecha).getDate(fecha);
            const fechaActual = new Date().getFullYear();
            const fechaActualm = new Date().getMonth() + 1;
            const fechaActuald = new Date().getDate();

            let edad = fechaActual - fecha_value;

            if ((fecha_valuem == fechaActualm) && (fecha_valued > fechaActuald)) {
                edad = (edad - 1);
            } else
                if (fecha_valuem > fechaActualm) {
                    edad = (edad - 1);
                }
            $("#edadcalculada").val(edad);
        }
        else {
            $("#edadcalculada").val(null);
        }
	}

