    function customFunction(input) {

        if (input.attr('name') === "IdEmpleado" && input.val() === "0") {
            return false;
        }

    }


    function GetDataUpload(e) {
        debugger;
        var validator = $("#frmEmployees").kendoValidator().data("kendoValidator")
        if (validator.validate()) {
            var displayedData1 = $("#gridEmployeeSalary").data().kendoGrid.dataSource.view();
            var id = 0;
            if ($("#IdEmpleado").val() == 0) {
                if (displayedData1.length > 0) {
                    for (var i = 0; i < displayedData1.length; i++) {
                        if (displayedData1[i].EmployeeSalaryId > id)
                            displayedData1[i].EmployeeSalaryId = 0;
                    }
                    id = 0;
                }
            }
            var setFechaEgreso = null;
            var setFechaFinContrato = null;
            if ($("#FechaEgreso").getKendoDatePicker().value() != null || $("#FechaEgreso").getKendoDatePicker().value() != undefined || $("#FechaEgreso").val() != "") {
                setFechaEgreso = $("#FechaEgreso").getKendoDatePicker().value().toJSON();
            }
            if ($("#FechaFinContrato").getKendoDatePicker().value() != null || $("#FechaFinContrato").getKendoDatePicker().value() != undefined || $("#FechaFinContrato").val() != "") {
                setFechaFinContrato = $("#FechaFinContrato").getKendoDatePicker().value().toJSON();
            }
            var _fechaNacimiento = $("#FechaNacimiento").getKendoDatePicker().value().toJSON()
            var _fechaIngreso = $("#FechaIngreso").getKendoDatePicker().value().toJSON()

            var dataObject = {
                'IdEmpleado': $("#IdEmpleado").val() === "" ? "0" : $("#IdEmpleado").val(),
                'NombreEmpleado': $("#NombreEmpleado").val(),
                'Correo': $("#Correo").val(),
                'FechaNacimiento': _fechaNacimiento,
                'FechaIngreso': _fechaIngreso,
                'Salario': Number($("#Salario").val()),
                'Identidad': $("#Identidad").val(),
                'FechaEgreso': setFechaEgreso,
                'Direccion': $("#Direccion").val(),
                'Genero': $("#Genero").data("kendoDropDownList").text(),
                'TipoSangre': $("#IdTipoSangre").data("kendoDropDownList").text(),
                'IdEstado': $("#IdEstado").val(),
                'IdState': $("#IdState").val(),
                'NombreContacto': $("#NombreContacto").val(),
                'TelefonoContacto': $("#TelefonoContacto").val(),
                'IdCountry': $("#IdCountry").val(),
                'IdCity': $("#IdCity").val(),
                'IdCurrency': $("#IdCurrency").val(),
                'IdActivoinactivo': $("#IdEstado").val(),
                'IdBank': $("#IdBank").val(),
                'CuentaBanco': $("#CuentaBanco").val(),
                'FechaFinContrato': setFechaFinContrato,
                'Telefono': $("#Telefono").val(),
                'Extension': $("#Extension").val(),
                'IdPuesto': $("#IdPuesto").val(),
                'IdSucursal': $("#IdSucursal").val(),
                'IdDepartamento': $("#IdDepartamento").val(),
                'IdTipoContrato': $("#IdTipoContrato").val(),
                'Notas': $("#Notas").val(),
                'IdBranch': $("#IdBranch").val(),
                'ApplicationUserId': 'FC405B7D-9FE3-43C9-97B5-D87A174CAB8A',
                'RTN': $("#RTN").val(),
                'Profesion': $("#Profesion").val(),
                'IdTipoPlanilla': $("#IdTipoPlanilla").val(),
                'BirthPlace': $("#BirthPlace").val(),
                '_EmployeeSalary': displayedData1
            };
            e.data = dataObject;
        }
    }

    var onBegin = function () {
        //alert("Begin");
        $("#btnEmployees").prop("disabled", true);
        var validator = $("#frmEmployees").data("kendoValidator");

        if (!validator.validate()) {
            $.toast({
                heading: 'Error',
                text: 'Complete los datos del formulario',
                position: 'top-right',
                loaderBg: '#ff6849',
                icon: 'error',
                hideAfter: 30000,
                stack: 6
            });


        };
    };



    var onComplete = function (context) {
        //alert("Complete");
        //$('#ModalEmployees').modal('hide');

    };

    var onSuccess = function (context) {
        //alert(context);
        console.log(context);
        $.toast({
            heading: 'Satisfactorio',
            text: 'Datos guardados correctamente.',
            position: 'top-right',
            loaderBg: '#ff6849',
            icon: 'success',
            hideAfter: 30000,
            stack: 6
        });
        RefrescarGrid();
        $('.dropify-clear').click();
        console.log(context);
        if (context.IdEmpleado == 0) {
            $('#ModalEmployees').modal('hide');

            $("#btnEmployees").prop("disabled", false);
            return;
        }

        $("#btnEmployees").prop("disabled", false);
         window.location.href = "@Url.Action("pvwEditEmployees", "Employees")?IdEmpleado=" + context.IdEmpleado;

    };

    var onFailed = function (context) {
        // alert(context);
        console.log(context);
        $.toast({
            heading: 'Error',
            text: 'Error al Crear Empleado',
            position: 'top-right',
            loaderBg: '#ff6849',
            icon: 'error',
            hideAfter: 30000,
            stack: 6
        });
        $("#btnEmployees").prop("disabled", false);

    };


    function EstadoName() {
        if ($("#IdEstado").data("kendoDropDownList").value() == 0) {
            $("#NombreEstado").val("");
        }
        else {
            var Estado = $("#IdEstado").data("kendoDropDownList").text()
            $("#NombreEstado").val(Estado);
        }
    }
    function PuestoName() {
        if ($("#IdPuesto").data("kendoDropDownList").value() == 0) {
            $("#NombrePuesto").val("");
        }
        else {
            var Estado = $("#IdPuesto").data("kendoDropDownList").text()
            $("#NombrePuesto").val(Estado);
        }
    }

</script>




