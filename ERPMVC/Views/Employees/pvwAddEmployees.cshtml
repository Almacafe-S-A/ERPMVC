<script>
    function AddSalarioEmpleado() {
        $('#myModalSalarioEmpleado').modal('show');
    }

    $(function () {
        var dateFormat = "dd/MM/yyyy",
            from = $("#FechaEgreso")
                .kendoDateTimePicker({
                    defaultDate: "+1w",
                })
                .on("change", function () {
                    console.log("Min", getDate(this));
                    to.kendoDateTimePicker("min", getDate(this));
                }),
            to = $("#FechaIngreso").kendoDateTimePicker({
                defaultDate: "+1w",
            })
                .on("change", function () {
                    from.kendoDateTimePicker("max", getDate(this));
                });

        function getDate(element) {
            var date;
            try {
                date = element.value;
            } catch (error) {
                date = null;
            }

            return date;
        }
    });

    $(function () {
        $("#FechaNacimiento").kendoDateTimePicker({
            depth: "month",
            start: "decade"
        });
    });
    //var getDate = function (input) {
    //    return new Date(input.date.valueOf());
    //}

    
    //Validaciones
    function SoloLetras(e) {
        key = e.keyCode || e.which;
        tecla = String.fromCharCode(key).toLowerCase();
        letras = " áéíóúabcdefghijklmnñopqrstuvwxyz";
        especiales = "8-37-39-46";

        tecla_especial = false
        for (var i in especiales) {
            if (key == especiales[i]) {
                tecla_especial = true;
                break;
            }
        }

        if (letras.indexOf(tecla) == -1 && !tecla_especial) {
            return false;
        }
    }


    function SoloNumeros(e) {
        key = e.keyCode || e.which;
        tecla = String.fromCharCode(key).toLowerCase();
        letras = "1234567890";
        especiales = "8-37-39-46";

        tecla_especial = false
        for (var i in especiales) {
            if (key == especiales[i]) {
                tecla_especial = true;
                break;
            }
        }

        if (letras.indexOf(tecla) == -1 && !tecla_especial) {
            return false;
        }
    }

    function SaveEmployees(e) {

