@*@model ERPMVC.Models.Employees*@
@model ERPMVC.DTO.EmployeesDTO
@using System.Security.Claims

@{
    ViewData["Title"] = "Empleado";
}

@{
    var messages = new Dictionary<string, string>() { { "custom", "Ingrese un valor correcto." },
{ "required","Valor requerido" }};

    var rules = new Dictionary<string, string>() { { "custom", "customFunction" } };
    ClaimsPrincipal permisos = (ClaimsPrincipal)ViewData["permisos"];
}

<script src="~/admp/assets/plugins/dropify/dist/js/dropify.min.js"></script>

<link rel="stylesheet" href="~/admp/assets/plugins/dropify/dist/css/dropify.min.css">


<script>
    $(document).on('load', '#FechaNacimiento', function () {
        GenerarEdad();
    });
    $(document).ready(function () {
        $("#TelefonoAlert").hide();
        $("#TelefonoContactoAlert").hide();
        $("#Telefono").kendoMaskedTextBox({
            mask: "(999) 0000-0000"
        });
        $("#TelefonoContacto").kendoMaskedTextBox({
            mask: "(999) 0000-0000"
        });
        $("#AlertSalario").prop("hidden", true);
        $("#AgregarSalarioCustom").prop("hidden", true)
        $('#Salario').val($('#Salario').val().replace(/\,/g, '.'));
        GenerarEdad();
        var data = [
            { text: "Masculino", value: "1" },
            { text: "Femenino", value: "2" }
        ];
        $("#IdGenero").kendoDropDownList({
            dataTextField: "text",
            dataValueField: "value",
            dataSource: data,
            index: 0,
            optionLabel: {
                text: "Seleccionar Género",
                value: ""
            }
        });
        var TipoSangre = [
            { text: "O-", value: "1" },
            { text: "O+", value: "2" },
            { text: "A-", value: "3" },
            { text: "A+", value: "4" },
            { text: "B-", value: "5" },
            { text: "B+", value: "6" },
            { text: "AB-", value: "7" },
            { text: "AB+", value: "8" },
        ];

        // create DropDownList from input HTML element
        $("#IdTipoSangre").kendoDropDownList({
            dataTextField: "text",
            dataValueField: "value",
            dataSource: TipoSangre,
            index: 0,
            optionLabel: {
                text: "Seleccionar Tipo de Sangre",
                value: ""
            }
            //change: onChange
        });
        if ("@Model.Genero" == "Masculino") {
            $("#IdGenero").data("kendoDropDownList").value(1);
            validarGenero();
        }
        else if("@Model.Genero" == "Femenino"){
            $("#IdGenero").data("kendoDropDownList").value(2);
            validarGenero();
        }

        if ("@Model.TipoSangre" == "O-") {
            $("#IdTipoSangre").data("kendoDropDownList").value(1);
            validarTipoSangre();
        }
        else if ("@Model.TipoSangre" == "O&#x2B;") {
            $("#IdTipoSangre").data("kendoDropDownList").value(2);
            validarTipoSangre();
        }
        else if ("@Model.TipoSangre" == "A-") {
            $("#IdTipoSangre").data("kendoDropDownList").value(3);
            validarTipoSangre();
        }
        else if ("@Model.TipoSangre" == "A&#x2B;") {
            $("#IdTipoSangre").data("kendoDropDownList").value(4);
            validarTipoSangre();
        }
        else if ("@Model.TipoSangre" == "B-") {
            $("#IdTipoSangre").data("kendoDropDownList").value(5);
            validarTipoSangre();
        }
        else if ("@Model.TipoSangre" == "B&#x2B;") {
            $("#IdTipoSangre").data("kendoDropDownList").value(6);
            validarTipoSangre();
        }
        else if ("@Model.TipoSangre" == "AB-") {
            $("#IdTipoSangre").data("kendoDropDownList").value(7);
            validarTipoSangre();
        }
        else if ("@Model.TipoSangre" == "AB&#x2B;") {
            $("#IdTipoSangre").data("kendoDropDownList").value(8);
            validarTipoSangre();
        }
        $("#GeneroAlert").hide();
        $("#TipoSangreAlert").hide();
        $("#IdentidadAlert").prop("hidden", true);
        $("#RTNAlert").prop("hidden", true);
    });

    function customFunction(input) {

        if (input.attr('name') === "BranchId" && input.val() === "0") {
            return false;
        }

    }


    function GetEmployeesId() {
        return { EmployeesId: $("#EmployeesId").val() }
    }

    function BackIndex() {
        window.location.href = '@Url.Action("Index","Employees")'
    }


</script>






<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">

                    <div class="modal-header">
                        <h1 class="modal-title" style="color:white">Empleado</h1>
                        <button type="button" class="close" onclick="BackIndex()">&times;</button>
                    </div>
                    <br />

                    <form id="frmEmployees" kendo-validator="true" asp-action="SaveEmployees" asp-controller="Employees"
                          data-ajax="true"
                          data-ajax-method="post" enctype="multipart/form-data"
                          data-ajax-begin="onBegin" data-ajax-complete="onComplete"
                          data-ajax-failure="onFailed" data-ajax-success="onSuccess"
                          class="validation-wizard wizard-circle">
                        <div class="form-body">
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="IdEmpleado" class=" control-label" style="width:100%">Id Empleado</label>
                                        <input type="text" asp-for="IdEmpleado" name="IdEmpleado" class="form-control" readonly style="min-width:100%" />
                                        <span asp-validation-for="IdEmpleado" class="text-danger"></span>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="NombreEmpleado" class=" control-label" style="width:100%">Nombre de empleado</label>
                                        <input type="text" asp-for="NombreEmpleado" name="NombreEmpleado" required data-required-msg="El Nombre del Empleado es requerido." class="form-control form-control-line" style="min-width:100%" maxlength="250" />
                                        <span asp-validation-for="NombreEmpleado" class="text-danger"></span>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Identidad" class=" control-label" style="width:100%">Identidad</label>
                                        <input type="text" asp-for="Identidad" name="Identidad" required data-required-msg="La Número de Identidad es requerido." onchange="ValidarIdentidad()" onkeypress="return SoloNumeros(event)" class="form-control form-control-line" autocomplete="off" style="min-width:100%" maxlength="15" />

                                        <span id="IdentidadAlert" class="text-danger"><span role="alert" class="k-icon k-i-warning"></span> El Número de Identidad ya Exíste!</span>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label asp-for="FechaNacimiento" class=" control-label" style="width:100%">Fecha nacimiento</label>

                                        <kendo-datepicker name="FechaNacimiento" style="width: 100%;" onchange="GenerarEdad();"
                                                          format="dd/MM/yyyy" max="@DateTime.Now"
                                                          data-val-required="La Fecha de Nacimiento es requerida."
                                                          value="Model.FechaNacimiento"/>
                                        <span asp-validation-for="FechaNacimiento" class="text-danger"></span>
                                    </div>
                                </div>

                                <div class="col-md-1">
                                    <div class="form-group">
                                        <label class=" control-label" style="width:100%">Edad</label>
                                        <input name="edadcalculada" id="edadcalculada" type="text" readonly autocomplete="off" class="form-control" style="min-width:100%" />
                                    </div>
                                </div>
                                <div class="col-sm-4 col-md-4">
                                    <div class="form-group">
                                        <label asp-for="BirthPlace" class=" control-label" style="width:100%">Lugar de Nacimiento</label>
                                        <input asp-for="BirthPlace" type="text" name="BirthPlace" required data-required-msg="El Lugar de Nacimiento es requerido." class="form-control" maxlength="100" autocomplete="off" style="min-width:100%" />
                                        <span asp-validation-for="BirthPlace" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Genero" class=" control-label">Género</label>
                                        <input id="IdGenero" asp-for="Genero" required data-required-msg="El Género es requerido." autocomplete="off" onchange="validarGenero()" style="min-width:100%" />
                                        <span asp-validation-for="Genero" class="text-danger"></span>
                                        <span id="GeneroAlert" class="text-danger"><span role="alert" class="k-icon k-i-warning"></span> El Género es requerido.</span>
                                    </div>
                                </div>
                            </div>

                            <input id="Genero" asp-for="GeneroName" hidden="hidden" data-validate="false" />

                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="TipoSangre" class=" control-label">Tipo de Sangre</label>
                                        <input id="IdTipoSangre" asp-for="TipoSangre" required data-required-msg="El Tipo de Sangre es requerido." autocomplete="off" onchange="validarTipoSangre()" style="min-width:100%" />
                                        @*<span asp-validation-for="TipoSangre" class="text-danger"></span>*@
                                        <span id="TipoSangreAlert" class="text-danger"><span role="alert" class="k-icon k-i-warning"></span> El Tipo de Sangre es requerido.</span>
                                    </div>
                                </div>

                                <input asp-for="TipoSangreName" id="TipoSangre" data-validate="false" hidden="hidden" />

                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Correo" class=" control-label" style="width:100%">Correo</label>
                                        <input type="email" asp-for="Correo" name="Correo" maxlength="150" placeholder="nombre@ejemplo.com" data-email-msg="Formato del Correo no valido." data-val-required="El correo es requerido." class="form-control" autocomplete="off" style="min-width:100%" />
                                        <span asp-validation-for="Correo" class="text-danger"></span>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Telefono" class=" control-label" style="width:100%">Teléfono</label>
                                        <input type="text" asp-for="Telefono" onchange="validarTelefono()" name="Telefono" required data-required-msg="El Número de Teléfono es requerido." class="form-control" autocomplete="off" style="min-width:100%" />
                                        <span asp-validation-for="Telefono" class="text-danger"></span>
                                        <span id="TelefonoAlert" class="text-danger"><span role="alert" class="k-icon k-i-warning"></span> Complete el número de teléfono.</span>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Extension" class=" control-label" style="width:100%">Extensión</label>
                                        <input type="text" asp-for="Extension" name="Extension"   onkeypress="return SoloNumeros(event)" class="form-control" maxlength="7" autocomplete="off" style="min-width:100%" />
                                        
                                    </div>
                                </div>
                                <div class="col-sm-4 col-md-4">
                                    <div class="form-group">
                                        <label asp-for="RTN" class=" control-label" style="width:100%">RTN</label>
                                        <input type="text" asp-for="RTN" name="RTN" required data-required-msg="El RTN es requerido." class="form-control" onchange="ValidarRTN()" onkeypress="return SoloNumeros(event)" maxlength="14" autocomplete="off" style="min-width:100%" />
                                        <span asp-validation-for="RTN" class="text-danger"></span>
                                        @*<span id="RTNAlert" class="text-danger">El Número de RTN ya Exíste!</span>*@
                                        <span id="RTNAlert" class="text-danger"><span role="alert" class="k-icon k-i-warning"></span> El Número de RTN ya Exíste!</span>
                                    </div>
                                </div>

                                <div class="col-sm-4 col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Profesion" class=" control-label" style="width:100%">Profesión</label>
                                        <input type="text" asp-for="Profesion" name="Profesion" required data-required-msg="La Profesión es requerida." class="form-control" maxlength="100" autocomplete="off" style="min-width:100%" />
                                        <span asp-validation-for="Profesion" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-4 col-md-4">
                                    <div class="form-group">
                                        <label asp-for="IdTipoPlanilla" class="control-label"></label>
                                        <kendo-dropdownlist name="IdTipoPlanilla"
                                                            for="@Model.IdTipoPlanilla"
                                                            filter="Kendo.Mvc.UI.FilterType.Contains"
                                                            option-label="Seleccionar Tipo Planilla"
                                                            datatextfield="TipoPlanilla"
                                                            datavaluefield="IdTipoPlanilla"
                                                            style="width: 100%;">
                                            <datasource type="DataSourceTagHelperType.WebApi" server-operation="true">
                                                <transport>
                                                    <read url="@Url.Action("GetTipoPlanillas", "TipoPlanillas")" />
                                                </transport>
                                            </datasource>
                                        </kendo-dropdownlist>
                                    </div>
                                </div>
                                <div class="col-sm-4 col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Notas" class=" control-label" style="width:100%">Notas</label>
                                        <textarea asp-for="Notas" name="Notas" rows="6" maxlength="250" class="form-control form-control-line" autocomplete="off" style="min-width:100%"></textarea>
                                        <span asp-validation-for="Notas" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="IdEstado" class="control-label"></label>
                                        <kendo-dropdownlist name="IdEstado" required data-required-msg="El Estado es requerido."
                                                            for="@Model.IdEstado"
                                                            filter="Kendo.Mvc.UI.FilterType.Contains"
                                                            option-label="Seleccionar Estado"
                                                            datatextfield="NombreEstado"
                                                            datavaluefield="IdEstado"
                                                            style="width: 100%;">
                                            <datasource type="DataSourceTagHelperType.WebApi" server-operation="true">
                                                <transport>
                                                    <read url="@Url.Action("GetGrupoEstadoUno", "Estados")" />
                                                </transport>
                                            </datasource>
                                        </kendo-dropdownlist>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="FechaIngreso" class=" control-label" style="width:100%">Fecha Ingreso</label>
                                        <kendo-datepicker name="FechaIngreso" style="width: 100%;"
                                                          format="dd/MM/yyyy hh:mm"
                                                          time-format="hh:mm"
                                                          data-val-required="La Fecha de Ingreso es requerida."
                                                          value="Model.FechaIngreso"/>
                                        <span asp-validation-for="FechaIngreso" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="FechaEgreso" class=" control-label" style="width:100%">Fecha de Egreso</label>
                                        <kendo-datepicker name="FechaEgreso" style="width: 100%;"
                                                          format="dd/MM/yyyy "
                                                          value="Model.FechaEgreso"/>
                                        <span asp-validation-for="FechaEgreso" class="text-danger"></span>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="IdDepartamento" class="control-label">Área</label>
                                        <kendo-dropdownlist name="IdDepartamento" required data-required-msg="El Área es requerida."
                                                            for="@Model.IdDepartamento"
                                                            filter="Kendo.Mvc.UI.FilterType.Contains"
                                                            option-label="Seleccionar Departamento"
                                                            datatextfield="NombreDepartamento"
                                                            datavaluefield="IdDepartamento" onchange="refreshpuesto();"
                                                            style="width: 100%;">
                                            <datasource type="DataSourceTagHelperType.WebApi" server-operation="true" page-size="9999999999">
                                                <transport>
                                                    <read url="@Url.Action("GetActivos", "Departamento")" />
                                                </transport>
                                            </datasource>
                                        </kendo-dropdownlist>
                                    </div>
                                </div>
                            </div>

                            <script>
                                function GetIdDepartamento() {

                                    return { 'IdDepartamento': $("#IdDepartamento").val() }
                                }

                                function refreshpuesto() {
                                    $("#IdPuesto").data("kendoDropDownList").dataSource.read();
                                }
                            </script>

                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="IdPuesto" class="control-label">Puesto</label>
                                        <kendo-dropdownlist name="IdPuesto" required data-required-msg="El Puesto es requerido."
                                                            for="@Model.IdPuesto"
                                                            filter="Kendo.Mvc.UI.FilterType.Contains"
                                                            option-label="Seleccionar puesto"
                                                            datatextfield="NombrePuesto"
                                                            datavaluefield="IdPuesto"
                                                            style="width: 100%;">
                                            <datasource type="DataSourceTagHelperType.WebApi" server-operation="true" page-size="9999999999">
                                                <transport>
                                                    <read url="@Url.Action("GetPuestoByIdDepartamento", "Puesto")" data="GetIdDepartamento" />
                                                </transport>
                                            </datasource>
                                        </kendo-dropdownlist>
                                    </div>
                                </div>




                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Salario" class=" control-label" style="width:100%">Salario</label>
                                        <div class="input-group">
                                            @*<input type="number" spinner="true" step="1" asp-for="Salario" name="Salario" required data-required-msg="El Salario es requerido." readonly autocomplete="off" class="form-control" style="min-width:70%" />*@
                                            <kendo-numerictextbox name="Salario" class="readonly"
                                                                  format="n2" spinners="false" decimals="2" tabindex="-1" enable="false"
                                                                  min="0" data-val-required="El Salario es requerido." data-min-msg="El Salario no puede ser negativo."
                                                                  style="width: 100%;text-align:right;padding-right: 3px;color:black;font-weight:500"
                                                                  max="999999999"
                                                                  step="1"
                                                                  value="Convert.ToDouble(Model.Salario)" />
                                            <span class="input-group-btn">
                                                <button type="button" class="btn btn-info" onclick="AddSalarioEmpleado()">AÑADIR</button>
                                            </span>
                                            <span asp-validation-for="Salario" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>

                                <input asp-for="QtySalary" type="number" name="QtySalary" data-validate="false" id="QtySalaryFrm" class="form-control" style="min-width:100%" hidden="hidden" />

                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="IdCurrency" class=" control-label" style="width:100%">Moneda salario</label>
                                        <kendo-dropdownlist name="IdCurrency" required data-required-msg="La Moneda es requerida."
                                                            for="@Model.IdCurrency"
                                                            filter="Kendo.Mvc.UI.FilterType.Contains"
                                                            option-label="Seleccionar Moneda"
                                                            datatextfield="CurrencyName"
                                                            datavaluefield="CurrencyId"
                                                            style="width: 100%;">
                                            <datasource type="DataSourceTagHelperType.WebApi" server-operation="true" page-size="9999999999">
                                                <transport>
                                                    <read url="@Url.Action("GetCurrencyActivos", "Currency")" />
                                                </transport>
                                            </datasource>
                                        </kendo-dropdownlist>
                                        <span asp-validation-for="IdCurrency" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-4">
                                    <script>
                                        function refreshdepartamento() {
                                            $("#IdState").data("kendoDropDownList").dataSource.read();
                                        }
                                    </script>
                                    <div class="form-group">
                                        <label asp-for="IdCountry" class="control-label">País</label>
                                        <kendo-dropdownlist name="IdCountry" required data-required-msg="El País es requerido."
                                                            for="@Model.IdCountry"
                                                            filter="Kendo.Mvc.UI.FilterType.Contains"
                                                            option-label="Seleccionar País"
                                                            datatextfield="Name"
                                                            datavaluefield="Id"
                                                            onchange="refreshdepartamento();"
                                                            style="width: 100%;">
                                            <datasource type="DataSourceTagHelperType.WebApi" server-operation="true" page-size="9999999999">
                                                <transport>
                                                    <read url="@Url.Action("GetActivos", "Country")" />
                                                </transport>
                                            </datasource>
                                        </kendo-dropdownlist>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <script>
                                        function GetCountryId() {

                                            return { 'CountryId': $("#IdCountry").val() }
                                        }

                                        function refreshmunicipio() {
                                            $("#IdCity").data("kendoDropDownList").dataSource.read();
                                        }
                                    </script>
                                    <div class="form-group">
                                        <label asp-for="IdState" class="control-label">Departamento</label>
                                        <kendo-dropdownlist name="IdState" required data-required-msg="El Departamento es requerido."
                                                            for="@Model.IdState"
                                                            filter="Kendo.Mvc.UI.FilterType.Contains"
                                                            option-label="Seleccionar Departamento"
                                                            datatextfield="Name"
                                                            datavaluefield="Id"
                                                            onchange="refreshmunicipio();"
                                                            style="width: 100%;" >
                                            <datasource type="DataSourceTagHelperType.WebApi" server-operation="true" page-size="9999999999">
                                                <transport >
                                                    <read url="@Url.Action("GetJsonActivos", "State")" data="GetCountryId"  />
                                                </transport>
                                            </datasource>
                                        </kendo-dropdownlist>
                                    </div>
                                </div>


                                <div class="col-md-4">
                                    <script>
                                        function GetCityId() {

                                            return { 'StateId': $("#IdState").val() }
                                        }
                                    </script>
                                    <div class="form-group">
                                        <label asp-for="IdCity" class="control-label">Ciudad</label>
                                        <kendo-dropdownlist name="IdCity" required data-required-msg="La Ciudad es requerida."
                                                            for="@Model.IdCity"
                                                            filter="Kendo.Mvc.UI.FilterType.Contains"
                                                            option-label="Seleccionar Ciudad"
                                                            datatextfield="Name"
                                                            datavaluefield="Id"
                                                            style="width: 100%;">
                                            <datasource type="DataSourceTagHelperType.WebApi" server-operation="true" page-size="9999999999">
                                                <transport>
                                                    <read url="@Url.Action("GetCityJsonActivos", "City")" data="GetCityId" />
                                                </transport>
                                            </datasource>
                                        </kendo-dropdownlist>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="Direccion" class=" control-label" style="width:100%">Dirección</label>
                                        <input type="text" asp-for="Direccion" name="Direccion" required data-required-msg="La Dirección es requerida." class="form-control" maxlength="500" autocomplete="off" style="min-width:100%" />
                                        <span asp-validation-for="Direccion" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label asp-for="IdBank" class="control-label">Banco</label>
