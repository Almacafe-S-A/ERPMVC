        var fecha_value = new Date(fecha).getFullYear(fecha);
        var fecha_valuem = new Date(fecha).getMonth(fecha) + 1;
        var fecha_valued = new Date(fecha).getDate(fecha);
        const fechaActual = new Date().getFullYear();
        const fechaActualm = new Date().getMonth() + 1;
