                                                                <transport>
                                                                    <read url="@Url.Action("RecibosPendientesLiquidar","GoodsReceived")" data="GetCustomerService" />
                                                                </transport>
                                                            </datasource>

                                                        </kendo-multiselect>
                                                    }
                                                    else
