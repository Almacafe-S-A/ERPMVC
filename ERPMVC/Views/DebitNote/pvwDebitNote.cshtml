<script>
    function BuscarFactura(e) {
        var dataObject = {
            InvoiceId: $("#FacturaId").val(),
            editar: 0
        };
        console.log("Id", dataObject);
                    $.ajax({
                        url: '@Url.Action("GetInvoiceById", "Invoice")',
                        method: 'POST',
                        datatype: "json",
                        contentType: 'application/json',
                        async: false,
                        data: JSON.stringify(dataObject),
                        success: function (result) {
                            console.log(result);
                            $("#Direccion").val(result.Direccion);
                            $("#DebitNoteName").val(result.CustomerName);
                            $("#RTN").val(result.RTN);
                            $("#Correo").val(result.Correo);
