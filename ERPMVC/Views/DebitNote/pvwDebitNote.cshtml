                            $("#CustomerRefNumber").val(result.CustomerRefNumber);
                            $("#ExpirationDate").kendoDateTimePicker({ value: new Date(result.ExpirationDate), dateInput: true })
                            $("#DeliveryDate").kendoDateTimePicker({ value: new Date(result.DeliveryDate), dateInput: true })
                            $("#OrderDate").kendoDateTimePicker({ value: new Date(result.OrderDate), dateInput: true })
                            $("#NoOCExenta").val(result.NoOCExenta);
                            $("#NoSAG").val(result.NoSAG);
