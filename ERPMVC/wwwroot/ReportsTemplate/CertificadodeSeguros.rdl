      <Query>
        <DataSourceName>ERP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select 
 cd.CustomerName,
 cl.BranchName
 cl.WarehouseId,
 cl.WarehouseName,
 cl.Amount,
 cl.SubProductName,
 Insurances.InsurancesName,
 cd.FechaCertificado


from CertificadoDeposito cd
inner join CertificadoLine cl on cl.IdCD = cd.IdCD
inner join InsuredAssets ia on ia.WarehouseId = cl.WarehouseId
inner join InsurancePolicy ins on ins.InsurancePolicyId = ia.InsurancePolicyId 
inner join Insurances on Insurances.InsurancesId = ins.InsurancesId
where ins.InsurancesId  = @InsuranceId and cd.CustomerId = @CustomerId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@InsuranceId">
            <Value>=Parameters!InsuranceId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerId">
            <Value>=Parameters!CustomerId.Value</Value>
