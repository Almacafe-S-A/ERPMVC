    <DataSet Name="Detalle">
      <Query>
        <DataSourceName>ERP</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Estado">
            <Value>=Parameters!Estado.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaInicio">
            <Value>=Parameters!FechaInicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaFin">
            <Value>=Parameters!FechaFin.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerId">
            <Value>=Parameters!CustomerId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SucursalId">
            <Value>=Parameters!SucursalId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  InvoicePayments.Id
  ,InvoicePayments.FechaPago
  ,InvoicePayments.BranchId
  ,InvoicePayments.CustomerId
  ,InvoicePayments.CustomerName  
  ,InvoicePayments.Sinopsis
  ,InvoicePayments.MontoPagado
  ,InvoicePayments.Estado
  ,InvoicePayments.UsuarioCreacion
  ,InvoicePayments.CuentaBancariaId
  ,AccountManagement.BankName
  ,AccountManagement.[Description]
  ,AccountManagement.AccountNumber
  ,CASE WHEN InvoicePayments.Estado LIKE '%Anulado%' THEN InvoicePayments.CustomerName+' ###ANULADO###' ELSE InvoicePayments.CustomerName END AS CLIENTE
  ,CASE WHEN InvoicePayments.Id LIKE '%Interna%' THEN STR(InvoicePayments.Id) ELSE InvoicePayments.Id END AS DEI
  ,CASE WHEN InvoicePayments.Estado LIKE '%Anulado%' THEN 0 ELSE InvoicePayments.MontoPagado END AS AMOUNT
FROM InvoicePayments
INNER JOIN AccountManagement ON InvoicePayments.CuentaBancariaId = AccountManagement.AccountManagementId
WHERE ((InvoicePayments.Estado = 'Aprobado' AND @Estado = '1') OR (InvoicePayments.Estado = 'Anulado' AND @Estado = '2') OR (InvoicePayments.Estado &lt;&gt;'En Revisión' AND @Estado = '0'))

AND InvoicePayments.FechaPago BETWEEN @FechaInicio AND dateadd(Day,1, @FechaFin)
  AND (InvoicePayments.CustomerId = @CustomerId OR @CustomerId = 0)
  AND (InvoicePayments.BranchId = @SucursalId OR @SucursalId = 0)</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <SelectedColumns>
              <ColumnExpression ColumnOwner="Invoice" ColumnName="InvoiceId" />
              <ColumnExpression ColumnOwner="Invoice" ColumnName="InvoiceDate" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="InvoiceLineId" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="ProductId" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="ProductName" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="SubProductId" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="SubProductName" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="UnitOfMeasureId" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="UnitOfMeasureName" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="SubTotal" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="TaxAmount" />
              <ColumnExpression ColumnOwner="InvoiceLine" ColumnName="Total" />
              <ColumnExpression ColumnOwner="Invoice" ColumnName="CustomerId" />
              <ColumnExpression ColumnOwner="Invoice" ColumnName="CustomerName" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="Invoice" />
              <Join Type="INNER JOIN" ColumnOwner="InvoiceLine">
                <On>
                  <From ColumnOwner="Invoice" ColumnName="InvoiceId" />
                  <To ColumnOwner="InvoiceLine" ColumnName="InvoiceId" />
                </On>
              </Join>
            </Joins>
          </QueryDefinition>
        </rd:DesignerState>
      </Query>
